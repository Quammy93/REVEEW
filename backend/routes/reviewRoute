const express = require("express");
const router = express.Router();

const {
  getAlReviews,
  getSingleReview,
  deleteReview,
  updateReview,
  createReview,
} = require("../controllers/reviewController");

router.route("/").post(createReview).get(getAlReviews);
router
  .route("/modify/:id")
  .patch(updateReview)
  .delete(deleteReview)
  .get(getSingleReview);

  module.exports=router